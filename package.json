{
  "name": "raylib",
  "version": "0.8.1",
  "description": "Node.js bindings for raylib.",
  "main": "index.js",
  "types": "./src/generated/node-raylib.d.ts",
  "scripts": {
    "start": "bin/node-raylib examples/core/core_basic_window.js",
    "benchmark": "bin/node-raylib examples/textures/textures_bunnymark.js",
    "test": "npx -y jest",
    "posttest": "npx -y standard --fix",
    "postinstall": "node tools/postinstall.js || npm run compile",
    "clean": "rm -rf build",
    "compile": "npm i --no-save node-addon-api && npx -y cmake-js compile",
    "prepkg": "npm run build",
    "pkg": "node tools/pkg.js",
    "gen:code": "node tools/generate.js",
    "ci:compile": "npm i --no-save node-addon-api cmake-js && cmake-js compile",
    "ci:test": "npm i --no-save jest && jest",
    "ci:format": "npm i --no-save standard && standard --fix"
  },
  "repository": "github:RobLoach/node-raylib",
  "engines": {
    "node": ">=10"
  },
  "bin": "./bin/node-raylib",
  "man": "./man/node-raylib.1",
  "keywords": [
    "raylib"
  ],
  "author": {
    "name": "Rob Loach",
    "url": "https://robloach.net"
  },
  "license": "Zlib",
  "files": [
    "docs/*",
    "bin/*",
    "man/node-raylib.1",
    "src/*",
    "src/lib/*",
    "vendor/*",
    "CMakeLists.txt",
    "index.js",
    "index.d.ts",
    "package.json",
    "README.md",
    "LICENSE.md",
    "CHANGELOG.md"
  ],
  "bugs": {
    "url": "https://github.com/RobLoach/node-raylib/issues"
  },
  "homepage": "https://github.com/RobLoach/node-raylib",
  "dependencies": {
    "cross-fetch": "^3.1.5",
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "archiver": "^5.3.0",
    "pkg": "^5.2.1"
  }
}
